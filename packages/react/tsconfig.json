{
    "extends": "../../tsconfig.base.json",
    "compilerOptions": {
        "moduleResolution": "node",
        "outDir": "./_esm",
        "noEmit": false,
        "jsx": "react",
        "tsPlusConfig": "../../tsplus.config.json",
        "baseUrl": "./",
        "rootDir": "./_src",
        "paths": {
            "@effect-ts-app/react/*": [
                "./_src/*"
            ],
            "@effect-ts-app/react": [
                "./_src"
            ],
        }
    },
    "include": [
        "./_src"
    ],
    "exclude": [
        "./dist",
        "node_modules",
        "build",
        "lib",
        "dist",
        "**/*.d.ts.map"
    ]
}