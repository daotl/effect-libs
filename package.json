{
  "name": "@daotl-effect/libs",
  "private": true,
  "pnpm": {
    "patchedDependencies": {
      "@typescript-eslint/scope-manager@5.57.0": "patches/@typescript-eslint__scope-manager@5.57.0.patch",
      "eslint-plugin-codegen@0.17.0": "patches/eslint-plugin-codegen@0.17.0.patch",
      "typescript@5.0.0-tsplus.20230224": "patches/typescript@5.0.0-tsplus.20230224.patch",
      "@effect/language-service@0.0.19": "patches/@effect__language-service@0.0.19.patch",
      "date-fns@2.29.3": "patches/date-fns@2.29.3.patch",
      "@fp-ts/optic@0.6.0": "patches/@fp-ts__optic@0.6.0.patch"
    }
  },
  "scripts": {
    "all": "pnpm -r --filter=!./packages/pnpm-singleton",
    "ci:version": "pnpm changeset version",
    "ci:publish": "pnpm all publish --access=public",
    "preinstall": "npx only-allow pnpm",
    "clean": "pnpm all clean",
    "clean-dist": "pnpm -r clean-dist",
    "upgrade": "echo hi",
    "autofix": "NODE_OPTIONS=--max-old-space-size=6144 pnpm -r autofix",
    "lint:all": "pnpm -r lint",
    "lint:fix:all": "pnpm -r lint:fix",
    "lint": "rome format . && rome check .",
    "lint:fix": "rome format --write . && rome check --apply .",
    "eslint": "pnpm -r eslint",
    "circular:dist": "pnpm -r circular:dist",
    "test": "pnpm -r test",
    "testsuite": "pnpm -r testsuite",
    "up-all": "pnpm --recursive update",
    "up-ncu": "pnpm run update && pnpm i",
    "update": "ncu -u && pnpm -r ncu -u",
    "ncu": "ncu",
    "eslint:watch": "ESLINT_TS=1 esw -w --changed --clear --ext ts,tsx",
    "packages": "pnpm -r --workspace-concurrency 1",
    "circular-packages": "pnpm packages circular",
    "lint-packages": "pnpm packages lint",
    "test-packages": "pnpm packages test:run",
    "testsuite-packages": "pnpm packages testsuite",
    "watch-packages": "pnpm packages build && pnpm packages watch",
    "build": "effect-app-cli packagejson-packages tsc --build ./tsconfig.all.json",
    "watch": "pnpm build:tsc --watch",
    "rebuild": "pnpm -r clean && pnpm -r build",
    "nnm": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' + && pnpm i"
  },
  "resolutions": {
    "@effect/data": "^0.10.2",
    "@effect/io": "^0.17.0",
    "@types/node": "~18.11.12",
    "date-fns": "^2.29.3",
    "http-proxy": "npm:http-proxy-node16@1.0.0",
    "typescript": "https://cdn.jsdelivr.net/npm/@tsplus/installer@0.0.171/compiler/typescript.tgz"
  },
  "dependencies": {
    "@effect-app/core": "^0.39.1",
    "@effect-app/infra": "^0.45.1",
    "@effect-app/infra-adapters": "^0.33.1",
    "@effect-app/prelude": "^0.34.1",
    "@effect-app/schema": "^0.31.1",
    "@effect/data": "^0.10.2",
    "@effect/io": "^0.17.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-transform-modules-commonjs": "^7.21.2",
    "@changesets/cli": "^2.26.1",
    "@daotl/rome-config": "0.0.1-alpha.4",
    "@effect-app/cli": "^0.9.0",
    "@effect-app/compiler": "^0.19.0",
    "@effect-app/pnpm-singleton": "workspace:*",
    "@effect/language-service": "^0.0.19",
    "@tsconfig/strictest": "^2.0.0",
    "@tsplus/installer": "0.0.171",
    "@types/lodash": "^4.14.192",
    "@types/node": "~18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@typescript-eslint/scope-manager": "^5.57.0",
    "babel-plugin-replace-import-extension": "^1.1.3",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "enhanced-resolve": "^5.12.0",
    "eslint": "^8.37.0",
    "eslint-plugin-codegen": "^0.17.0",
    "eslint-watch": "^8.0.0",
    "history": "^5.3.0",
    "http-proxy": "npm:http-proxy-node16@^1.0.0",
    "json5": "^2.2.3",
    "madge": "^6.0.0",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.22",
    "npm-check-updates": "^16.10.0",
    "rome": "^12.0.0",
    "ts-node": "^10.9.1",
    "ts-transform-paths": "^3.0.0",
    "tsc-watch": "^6.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "https://cdn.jsdelivr.net/npm/@tsplus/installer@0.0.171/compiler/typescript.tgz",
    "typescript-transform-paths": "^3.4.6",
    "vite": "^4.2.1",
    "vitest": "^0.29.8"
  }
