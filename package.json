        "packages": "pnpm -r --workspace-concurrency 1",
        "circular-packages": "pnpm packages circular",
        "lint-packages": "pnpm packages lint",
        "test-packages": "pnpm packages test:run",
        "testsuite-packages": "pnpm packages testsuite",
        "testsuite-packages-manual": "cd packages/boilerplate-prelude && pnpm testsuite && cd ../types && pnpm testsuite && cd ../client && pnpm testsuite && cd ../infra && pnpm testsuite && cd ../messages && pnpm testsuite && cd ../vue && pnpm testsuite",
        "watch-packages": "pnpm build-packages && pnpm -r --filter \"packages/*\" watch",
        "build": "NODE_OPTIONS=--max-old-space-size=6144 tsc --build ./tsconfig.all.json",
        "rebuild": "pnpm -r clean && pnpm -r build",
        "nnm": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' + && pnpm i"
