        "ncu": "ncu",
        "build-packages": "cd packages/boilerplate-prelude && pnpm build && cd ../types && pnpm build && cd ../client && pnpm build && cd ../vue && pnpm build && cd ../infra && pnpm build && cd ../messages && pnpm build && cd ../../packages/eslint && pnpm build",
