{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@effect-ts-app/core/*": [
        "packages/core/*"
      ],
      "@effect-ts-app/core": [
        "packages/core"
      ],
      "@effect-ts-app/infra/*": [
        "packages/infra/*"
      ],
      "@effect-ts-app/infra": [
        "packages/infra"
      ],
      "@effect-ts-app/fluent/*": [
        "packages/fluent/*"
      ],
      "@effect-ts-app/fluent": [
        "packages/fluent"
      ],
      "@effect-ts-app/prelude/*": [
        "packages/prelude/*"
      ],
      "@effect-ts-app/prelude": [
        "packages/prelude"
      ]
    },
    "target": "ES2020",
    "module": "ESNext"
  }
}